pronounce
- c -> ts
- ĉ -> ch
- ĝ -> judge [d3]
- ĥ -> loch
- ĵ -> zh
- ŝ -> sh
- û -> w

parts of speech
- -o noun
- -a adjective
- -e adverb
- -i infinitive
- -is past
- -as present
- -os future
- -u jussive

personal pronouns (-n to make noun)
- mi ni
- vi
- li/ŝi/ĝi ili
- oni -> one
- si -> self

conjunctions and adverbs
- kaj -> and
- ke -> that
- sed -> but
- nun -> now
- ne -> not

prepositions
- por -> for
- pro -> for
- al -> to
- el -> from, out of
- de -> of, from
- da -> of (follows quantity or measure)
- en -> in
- sur -> on
- kun -> with
- ĉar -> because
- pri -> about
- per -> by, with

verbs
- esti -> be
- diri -> say
- povi -> able to
- fari -> act, do, make, perform

correlatives
- ki- -> what
- ti- -> this
- i-  -> some
- ĉi- -> every
- neni-  -> no
- -a  quality (kind)  -- ahh kindness
- -al reason          -- al -> arabic -> religion -> reason to live
- -am time            -- am -> hangingbyamoment; A.M.
- -e  place           -- event myhouse
- -el manner          -- el scorcho hot sauce HOWhotisit
- -es association (belongs to; 's)   -- esmine
- -o  thing           -- bingothingo
- -om amount          -- homuchuwant  ommmsamounttonothingness
- -u  individual      -- u r n individual
- ajn  general
- ĉi   proximity
- for  distance

numbers
- nul
- unu
- du
- tri
- kvar
- kvin
- ses
- sep
- ok
- naû
- dek
- cent
- mil

nouns
- ŝipo -> ship
- insulo -> island

